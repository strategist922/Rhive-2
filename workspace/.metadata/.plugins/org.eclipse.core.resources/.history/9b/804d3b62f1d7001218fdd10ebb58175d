	   System.out.println(example_str.indexOf(0, 'c'));
	   String a = "2bin      每集"    ;
	   String b = a.replaceAll("[0-9[a-z]]", "  " ) ;
	   System.out.println(b.trim());
     /*	   
       for(int i=0 ; i< a.length() ;i++){
		   char ai = a.charAt(i);
		   if(isChinese(ai)){
		   System.out.println(ai);
		   }
	   }
	   System.out.println(ExtractChinese("niasas     asasq1 1234 中国"));
