package trs;
  /**
   * 从HDFS中载入数据到HBase.
   * @author ia
   *
   */
public class LoadDataToHBase {
    private String url ;

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	@Override
	public String toString() {
		return "LoadDataToHBase [url=" + url + "]";
	}

	public LoadDataToHBase(String url) {
		this.url = url;
	}
    
	public boolean load( String url ){
		
	}
	 
}
